server {
    autoindex on
	errorpages 404 src/pages/errorpages/error_404.html
	errorpages 503 src/pages/errorpages/error_503.html
	listen 8080
    methodes GET AGAIN
    server_names littleCloud
    client_size 42
    template src/pages/template.html
    timeoutCgi  10
    location /about {
        root src/pages/
        redirection about.html
    }
    location /about {
        root ./
        methodes GET
        redirection /pages
    }
    location / {
        methodes GET POST DELETE
        root ./
        redirection src/pages/index.html
        cgi_path   cgiFiles/
        cgi_extention  .cgi .py sdfdfs
        template src/pages/template.html
    }
    location /about {
        root src/pages/
        redirection about.html
    }
    location /cgi {
        root ./
        redirection cgiFiles/test.cgi
        cgi_path   cgiFiles/
        cgi_extention   .cgi
    }
    location /cgi/test {
        root ./
        redirection cgiFiles/cgi_tester.cgi
        cgi_path   cgiFiles/
        cgi_extention   .cgi
    }
    location /cgi/env {
        root ./
        methodes POST GET
        redirection cgiFiles/printEnv.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /listUpload {
        root ./
        redirection cgiFiles/listUpload.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /cgi/post {
        root ./
        methodes POST 
        redirection cgiFiles/post.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /favicon.ico {
        root ./
        methodes GET
        redirection src/pages/favicon.ico
    }
    location /download {
        root ./
        methodes GET
        redirection cgiFiles/getUpload.py
        cgi_path   cgiFiles/
        cgi_extention .py
    }
    location /arrow.png {
        root ./
        methodes GET
        redirection src/pages/arrow.png
    }
}


server {
    autoindex on
    template src/pages/template.html
	errorpages 505 src/pages/errorpages/error_505.html
    methodes GET POST DELETE
	listen 8081
    timeoutCgi  10
    location / {
        root ./
        methodes GET POST
        redirection src/pages/index.html
    }
    location /favicon.ico {
        root ./
        methodes GET
        redirection src/pages/favicon.ico
    }
    server_names bigCloud
    client_size 43
}


server {
    autoindex on
    errorpages 505 src/pages/errorpages/error_505.html
	listen 8082
    client_size 43
    methodes GET POST DELETE
    timeoutCgi  10
    location /about {
        root ./
        redirection src/pages/about.html
    }
    location / {
        root ./
        methodes GET POST
        redirection src/pages/index2.html
    }
    location /form-styles.css {
        root ./
        methodes GET POST
        redirection src/pages/form-styles.css
    }
    location /script.js {
        root ./
        methodes GET POST
        redirection src/pages/js/script.js
    }
}
