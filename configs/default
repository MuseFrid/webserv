server {
    autoindex on
	errorpages 404 www/pages/errorpages/error_404.html
	errorpages 503 www/pages/errorpages/error_503.html
	listen 8080
    methodes GET AGAIN
    server_names littleCloud
    client_size 42
    template www/pages/template.html
    timeoutCgi  10
    root src
    location /about {
        root www/pages/
        redirection about.html
    }
    location /about {
        root ./
        methodes GET
        redirection /pages
    }
    location / {
        methodes GET POST DELETE
        root src/pages/
        redirection index.html
        cgi_path   cgiFiles/
        cgi_extention  .cgi .py sdfdfs
        template www/pages/template.html
    }
    location /about {
        root www/pages/
        redirection about.html
    }
    location /cgi {
        root ./
        redirection cgiFiles/test.cgi
        cgi_path   cgiFiles/
        cgi_extention   .cgi
    }
    location /cgi/test {
        root ./
        redirection cgiFiles/cgi_tester.cgi
        cgi_path   cgiFiles/
        cgi_extention   .cgi
    }
    location /cgi/env {
        root ./
        methodes POST GET
        redirection cgiFiles/printEnv.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /listUpload {
        root ./
        redirection cgiFiles/listUpload.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /cgi/post {
        root ./
        methodes POST 
        redirection cgiFiles/post.py
        cgi_path   cgiFiles/
        cgi_extention   .cgi .py
    }
    location /favicon.ico {
        root ./
        methodes GET
        redirection www/pages/favicon.ico
    }
    location /errorpages/error_503.html {
        root src/pages/errorpages/
        methodes GET
        redirection error_503.html
    }
    location /download {
        root ./
        methodes GET
        redirection cgiFiles/getUpload.py
        cgi_path   cgiFiles/
        cgi_extention .py
    }
    location /arrow.png {
        root ./
        methodes GET
        redirection www/pages/arrow.png
    }
}


server {
    autoindex on
    template www/pages/template.html
	errorpages 505 www/pages/errorpages/error_505.html
    methodes GET POST DELETE
	listen 8081
    timeoutCgi  10
    root src
    location / {
        root ./
        methodes GET POST
        redirection www/pages/index.html
    }
    location /favicon.ico {
        root ./
        methodes GET
        redirection www/pages/favicon.ico
    }
    server_names bigCloud
    client_size 43
}


server {
    autoindex on
    errorpages 505 www/pages/errorpages/error_505.html
	listen 8082
    client_size 43
    methodes GET POST DELETE
    timeoutCgi  10
    root src
    location /about {
        root ./
        redirection www/pages/about.html
    }
    location / {
        root ./
        methodes GET POST
        redirection www/pages/index2.html
    }
    location /form-styles.css {
        root ./
        methodes GET POST
        redirection www/pages/form-styles.css
    }
    location /script.js {
        root ./
        methodes GET POST
        redirection www/pages/js/script.js
    }
}
