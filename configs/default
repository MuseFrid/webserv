server {
	errorpages 503 src/pages/errorpages/error_503.html
	listen 8080
    server_names littleCloud
    client_size 42
    location /about {
        methodes GET POST AGAIN
        redirection /pages/about.html
    }
    location / {
        methodes GET POST
        redirection /pages/index.html
    }
}
server {
	errorpages 404 src/pages/errorpages/error_404.html
	listen 8081
    location / {
        methodes GET POST
        redirection /pages/index.html
    }
    server_names bigCloud
    client_size 43
}
